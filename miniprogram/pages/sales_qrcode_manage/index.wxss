/**index.wxss**/
.title{
  font-size: 20px;
  font-weight: 600;
  background-color: whitesmoke;
  width: 80%;
  height: 40px;
  text-align: center;
  border-radius: 20px;
  line-height: 40px;
  margin-bottom: 30px;
}

.code_box_record_detail{
  font-size: 16px;
}

.box_container{
  margin-top: 10px;
  background-color: whitesmoke;
  width: 100%;
  height: 80px;
  border-radius: 10px;
}

.box{
  width: 80%;
}

.name{
  margin-top: 10px;
  margin-left: 10px;
}

.code_box_record {
  display: flex;
}

.code_box_record_title {
  width: 33%;
  font-size: 26rpx;
  color: rgba(0, 0, 0, 0.5);
  padding: 20rpx 0;
}

.code_box_record_detail {
  width: 33%;
  font-size: 26rpx;
  padding: 20rpx 0;
}

.code_box_record_detail_look{
  width: 10%;
  font-size: 26rpx;
  padding: 20rpx 0;
  color: green;
}